# Copie este arquivo para .env.docker e preencha com suas credenciais
# cp .env.docker.example .env.docker

# Application
NODE_ENV=development
PORT=3000
APP_NAME=ChinaFacil
APP_URL=http://host.docker.internal:3000

# Database - Main (host.docker.internal para acessar MySQL da maquina host)
DATABASE_URL="mysql://user:password@host.docker.internal:3306/chinafacil"

# Database - NCM/Impostos
DB_HOST_NCM_IMPOSTOS=your-rds-host.rds.amazonaws.com
DB_PORT_NCM_IMPOSTOS=3306
DB_DATABASE_NCM_IMPOSTOS=extension
DB_USERNAME_NCM_IMPOSTOS=your_username
DB_PASSWORD_NCM_IMPOSTOS=your_password

# Redis
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=

# JWT
JWT_SECRET=your_jwt_secret_here
JWT_EXPIRES_IN=7d
